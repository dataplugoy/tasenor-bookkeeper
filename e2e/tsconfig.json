{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "baseUrl": ".",
    "moduleResolution": "node",
    "types": ["cypress", "cypress-xpath", "cypress-real-events"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "dist"
  },
  "include": [
    "cypress/**/*.ts"
  ]
}
