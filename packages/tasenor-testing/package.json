{
  "name": "@tasenor/testing",
  "version": "1.9.35",
  "description": "Testing tools for Tasenor project",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "repository": "git@github.com:dataplugoy/tasenor-bookkeeper.git",
  "author": "Tommi Ronkainen <tommi.ronkainen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -fr dist && tsc",
    "ci": "pnpm run lint",
    "fix": "eslint --fix 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "prepublish": "pnpm version patch",
    "release": "pnpm publish --access public --no-git-checks",
    "show-version": "jq .version < package.json",
    "test": "jest",
    "watch": "jest --onlyChanged --watch"
  },
  "dependencies": {
    "@tasenor/common": "workspace:*",
    "@tasenor/common-node": "workspace:*",
    "@tasenor/common-plugins": "workspace:*",
    "axios": "0.26.0",
    "base64-arraybuffer": "^1.0.2",
    "clone": "^2.1.2",
    "dayjs": "1.10.8",
    "deep-equal": "^2.2.2",
    "sprintf-js": "^1.1.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.1",
    "@types/clone": "^2.1.1",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^29.5.2",
    "@types/sprintf-js": "^1.1.2",
    "eslint-config-tasenor": "workspace:*",
    "jest": "^29.6.1",
    "jestconfig": "workspace:*",
    "react": "^17.0.2",
    "tsconfig": "workspace:*",
    "typescript": "^5.1.6"
  }
}
