{
  "name": "@tasenor/testing",
  "version": "1.11.59",
  "description": "Testing tools for Tasenor project",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "repository": "git@github.com:dataplugoy/tasenor-bookkeeper.git",
  "author": "Tommi Ronkainen <tommi.ronkainen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rm -fr dist && tsc",
    "ci": "pnpm run lint && pnpm run test && pnpm run robot",
    "fix": "eslint --fix 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "nightly": "pnpm run lint && pnpm run test && pnpm run robot-nightly",
    "nightly3": "pnpm run cypress-nightly",
    "release": "pnpm version patch && pnpm publish --force --access public --no-git-checks",
    "release-minor": "pnpm version minor && pnpm publish --force --access public --no-git-checks",
    "robot": "rm -f robot/reports/* && robot/bin/test-ci --normal",
    "robot-nightly": "rm -f robot/reports/* && robot/bin/test-ci --nightly",
    "cypress-docker": "CYPRESS_URL=http://bookkeeper.localhost CYPRESS_API_URL=http://bookkeeper-api.localhost cypress open --browser chrome --e2e",
    "cypress-localhost": "CYPRESS_URL=http://localhost:${BOOKKEEPER_PORT:-7204} CYPRESS_API_URL=http://localhost:${BOOKKEEPER_API_PORT:-7205} cypress open --browser chrome --e2e",
    "cypress-nightly-localhost": "CYPRESS_MODE=nightly CYPRESS_URL=http://localhost:${BOOKKEEPER_PORT:-7204} CYPRESS_API_URL=http://localhost:${BOOKKEEPER_API_PORT:-7205} cypress open --browser chrome --e2e",
    "cypress": "CYPRESS_URL=http://localhost:${BOOKKEEPER_PORT:-7204} CYPRESS_API_URL=http://localhost:${BOOKKEEPER_API_PORT:-7205} cypress run",
    "cypress-nightly": "CYPRESS_MODE=nightly CYPRESS_URL=http://bookkeeper.localhost CYPRESS_API_URL=http://bookkeeper-api.localhost cypress run",
    "show-version": "jq .version < package.json",
    "test": "jest",
    "watch": "jest --onlyChanged --watch"
  },
  "dependencies": {
    "@tasenor/common": "workspace:*",
    "@tasenor/common-node": "workspace:*",
    "@tasenor/common-plugins": "workspace:*",
    "axios": "^1.6.7",
    "base64-arraybuffer": "^1.0.2",
    "clone": "^2.1.2",
    "dayjs": "1.10.8",
    "deep-equal": "^2.2.3",
    "sprintf-js": "^1.1.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@tasenor/config": "workspace:*",
    "@types/clone": "^2.1.4",
    "@types/deep-equal": "^1.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/sprintf-js": "^1.1.4",
    "csv-parse": "^5.5.3",
    "cypress": "^13.6.5",
    "cypress-real-events": "^1.12.0",
    "cypress-xpath": "^2.0.1",
    "eslint-config-tasenor": "workspace:*",
    "jest": "^29.7.0",
    "react": "^17.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
