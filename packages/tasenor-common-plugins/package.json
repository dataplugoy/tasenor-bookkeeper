{
  "name": "@tasenor/common-plugins",
  "version": "1.13.14",
  "description": "Shared common plugins of Tasenor project",
  "repository": "git@github.com:dataplugoy/tasenor-bookkeeper.git",
  "author": "Tommi Ronkainen <tommi.ronkainen@gmail.com>",
  "license": "MIT",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "ci": "pnpm run lint",
    "build": "rm -fr dist && tsc",
    "fix": "eslint --fix 'src/**/*.{ts,tsx}' 'data/**/*.mjs'",
    "lint": "eslint 'src/**/*.{ts,tsx}' 'data/**/*.mjs'",
    "nightly": "pnpm run ci",
    "nightly3": "pnpm run data",
    "data": "data/bin/build_all",
    "release": "pnpm version patch && pnpm publish --force --access public --no-git-checks",
    "release-minor": "pnpm version minor && pnpm publish --force --access public --no-git-checks",
    "show-version": "jq .version < package.json"
  },
  "dependencies": {
    "@tasenor/common": "workspace:*",
    "@tasenor/common-node": "workspace:*",
    "@tasenor/common-ui": "workspace:*",
    "clone": "^2.1.2",
    "currency-codes": "^2.1.0",
    "dayjs": "1.10.8",
    "sprintf-js": "^1.1.2",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.13.7",
    "@tasenor/config": "workspace:*",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.1",
    "eslint-config-tasenor": "workspace:*",
    "glob": "7.2.0",
    "i18next": "^23.2.11",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "react": "^17.0.2",
    "react-dom": "^17",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.14.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.13.7",
    "i18next": "^23.2.11",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "react": "^17.0.2",
    "react-dom": "^17",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.14.1"
  }
}
