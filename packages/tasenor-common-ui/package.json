    "@tasenor/config": "workspace:*",
    "@types/jest": "^29.5.12",
    "eslint-config-tasenor": "workspace:*",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.0",
    "i18next": "^21.4.0",
    "mobx": "6.4.2",
