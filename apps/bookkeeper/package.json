{
  "name": "@tasenor/bookkeeper",
  "version": "1.9.50",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "description": "Tasenor bookkeeper UI",
  "author": "Tommi Ronkainen <tommi.ronkainen@gmail.com>",
  "license": "AGPL",
  "scripts": {
    "build-test": "npx vite build",
    "ci": "pnpm run dev-initialize-plugins && pnpm run lint && pnpm run build-test",
    "dev-initialize-plugins": "DOTENV_CONFIG_PATH=`pwd`/.env.local npx tsx -r dotenv/config bin/initialize-plugins.mjs",
    "docker-vite": "DOTENV_CONFIG_PATH=`pwd`/.env.local npx vite preview --port ${PORT:=7204}",
    "dev-vite": "DOTENV_CONFIG_PATH=`pwd`/.env.local npx vite --port ${PORT:=7204}",
    "dev-ui-plugin-server": "DOTENV_CONFIG_PATH=`pwd`/.env.local npx tsx watch --clear-screen=false -r dotenv/config ./src/plugin-server/ui-plugin-server.ts",
    "dev": "pnpm run dev-initialize-plugins && pnpm run dev-ui-plugin-server & pnpm run dev-vite",
    "docker": "pnpm run dev-initialize-plugins && pnpm run dev-ui-plugin-server & pnpm run dev-vite",
    "fix": "eslint --fix 'src/**/*.ts{,x}'",
    "lint": "eslint 'src/**/*.ts{,x}'",
    "release": "pnpm version patch",
    "show-version": "jq .version < package.json"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.22.7",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.13.7",
    "@tasenor/common": "workspace:*",
    "@tasenor/common-node": "workspace:*",
    "@tasenor/common-ui": "workspace:*",
    "@vitejs/plugin-react": "^4.0.3",
    "clone": "^2.1.2",
    "dayjs": "1.10.8",
    "detect-browser": "^5.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "i18next": "^23.2.11",
    "jwt-decode": "^3.1.2",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17",
    "react-i18next": "^13.0.2",
    "react-router-dom": "^6.14.1",
    "react-router-prop-types": "^1.0.5",
    "react-world-flags": "^1.5.1",
    "safer-eval": "^1.3.6",
    "sprintf-js": "^1.1.2",
    "tsx": "^3.12.7",
    "vite": "^4.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "@types/react": "^17.0.62",
    "@types/react-dom": "^17",
    "eslint-config-tasenor": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^5.1.6"
  }
}
